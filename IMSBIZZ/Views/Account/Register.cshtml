@model IMSBIZZ.Models.RegisterViewModel
@{
    ViewBag.Title = "Register";
    Layout = null;
}

}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | Registration Page</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Scripts/adminlte/components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Scripts/adminlte/components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Scripts/adminlte/components/Ionicons/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/adminlte/css/AdminLTE.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/Scripts/adminlte/plugins/iCheck/square/blue.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition register-page">
    @using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
    {
        @Html.AntiForgeryToken()
        <div class="register-box" style="width:600px;">
            @*<div class="register-logo">
                    <a href="@Url.Action("Dashboardv2", "Dashboard")"><b>Admin</b>LTE</a>
                </div>*@
            <div class="register-box-body">
                <p class="login-box-msg text-center"><h2 class="text-center ">Register a new membership</h2></p>
                <br />
                <form action="@Url.Action("Dashboardv2", "Dashboard")" method="post">
                    <div class="form-group has-feedback">
                        @Html.TextBoxFor(m => m.FirstName, new { placeholder = "First Name", @class = "form-control" })
                        <span class="glyphicon glyphicon-user form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.FirstName)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.TextBoxFor(m => m.LastName, new { placeholder = "Last Name", @class = "form-control" })
                        <span class="glyphicon glyphicon-user form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.LastName)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.TextBoxFor(m => m.CompanyName, new { placeholder = "Company Name", @class = "form-control" })
                        <span class="glyphicon glyphicon-user form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.CompanyName)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.TextBoxFor(m => m.Email, new { placeholder = "Email", @class = "form-control" })
                        <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.Email)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.PasswordFor(m => m.Password, new { placeholder = "Password", @class = "form-control" })
                        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.Password)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.PasswordFor(m => m.ConfirmPassword, new { placeholder = " Confirm Password", @class = "form-control" })
                        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.ConfirmPassword)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.DropDownListFor(m => m.CountryId, ViewBag.CountryList as List<SelectListItem>, new { @class = "form-control" })
                        <span class="glyphicon glyphicon-log-in form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.CountryList)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.TextBoxFor(m => m.MobileNo, new { placeholder = "Mobile No", @class = "form-control" })
                        <span class="glyphicon glyphicon-earphone form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.MobileNo)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.TextBoxFor(m => m.StartDate, new { placeholder = "Start Date", @class = "form-control" })
                        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.StartDate)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.TextBoxFor(m => m.EndDate, new { placeholder = "End Date", @class = "form-control" })
                        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.EndDate)
                    </div>
                    <div class="form-group has-feedback">
                        @Html.TextBoxFor(m => m.ReferenceMobileNo, new { placeholder = "Reference Mobile No", @class = "form-control" })
                        <span class="glyphicon glyphicon-phone form-control-feedback"></span>
                        @Html.ValidationMessageFor(x => x.ReferenceMobileNo)
                    </div>
                    <div class="row">
                        <div class="col-xs-8">
                            <div class="checkbox icheck">
                                <label>
                                    <input type="checkbox"> I agree to the <a href="#">terms</a>
                                </label>
                            </div>
                        </div>
                        <!-- /.col -->
                        <div class="col-xs-4">

                            <button type="submit" class="btn btn-primary btn-block btn-flat">Submit</button>
                        </div>
                        <!-- /.col -->
                    </div>
                </form>
                <div class="social-auth-links text-center">
                    <p>- OR -</p>
                    @*<a href="#" class="btn btn-block btn-social btn-facebook btn-flat">
                            <i class="fa fa-facebook"></i> Sign up using
                            Facebook
                        </a>
                        <a href="#" class="btn btn-block btn-social btn-google btn-flat">
                            <i class="fa fa-google-plus"></i> Sign up using
                            Google+
                        </a>*@
                </div>
                <a href="@Url.Action("Login", "Examples")" class="text-center">I already have a membership</a>
            </div>
            <!-- /.form-box -->
        </div>
        <!-- /.register-box -->
        <!-- jQuery 3 -->
        <script src="~/Scripts/adminlte/components/jquery/dist/jquery.min.js"></script>
        <!-- Bootstrap 3.3.7 -->
        <script src="~/Scripts/adminlte/components/bootstrap/dist/js/bootstrap.min.js"></script>
        <!-- iCheck -->
        <script src="~/Scripts/adminlte/plugins/iCheck/icheck.min.js"></script>
        <script type="text/javascript">
            $(document).ready(function () {
                $('input').iCheck({
                    checkboxClass: 'icheckbox_square-blue',
                    radioClass: 'iradio_square-blue',
                    increaseArea: '20%' /* optional */
                });
            });
        </script>
    }

    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }<h2>@ViewBag.Title.</h2>
</body>
</html>

